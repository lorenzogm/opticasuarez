{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.opticasuarezjaen.es"
        }
      ],
      "destination": "https://opticasuarezjaen.es/$1",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/((?!.*\\.).*)$",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html; charset=utf-8"
        }
      ]
    }
  ]
}
